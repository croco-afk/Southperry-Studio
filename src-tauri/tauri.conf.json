{
  "productName": "MapleLens",
  "version": "1.0.0",
  "identifier": "com.maplelens.app",
  "build": {
    "beforeDevCommand": "cd src && npm run dev",
    "beforeBuildCommand": "cd src && npm run build",
    "devUrl": "http://localhost:8041",
    "frontendDist": "../src/dist"
  },
  "app": {
    "windows": [
      {
        "title": "MapleLens",
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}